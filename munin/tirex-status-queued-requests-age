use strict;
use warnings;

use Tirex::Munin::Status::QueuedRequestsAge;

#-----------------------------------------------------------------------------

my $config_file = $Tirex::TIREX_CONFIGFILE;
Tirex::Config::init($config_file);

#-----------------------------------------------------------------------------

my @z = ('0-9', 10 .. 17);

Tirex::Munin::Status::QueuedRequestsAge->new( z => \@z )->do();


